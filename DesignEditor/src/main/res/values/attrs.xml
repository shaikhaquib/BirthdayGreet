<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StickerView">
        <attr name="showIcons" format="boolean" />
        <attr name="showBorder" format="boolean" />
        <attr name="bringToFrontCurrentSticker" format="boolean" />
        <attr name="borderColor" format="color"/>
        <attr name="borderAlpha" format="integer"/>
    </declare-styleable>

    <declare-styleable name="TedBottomPickerImageView">
        <attr name="foreground" format="reference"/>
        <attr name="fit_mode" format="string"/>
    </declare-styleable>



    <declare-styleable name="TedBottomPickerSquareView">
        <attr name="matchHeightToWidth" format="boolean"/>
        <attr name="matchWidthToHeight" format="boolean"/>
    </declare-styleable>
</resources>